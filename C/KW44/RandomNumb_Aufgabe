

#include <stdio.h>
#include <stdlib.h>

/*
 * 
 */
int* getRandomNums(unsigned int n) {
    int *nums = malloc(n * sizeof (int));

    if (nums == NULL) exit(1);

    for (int i = 0; i < n; i++) {
        *(nums + i) = rand() % 100 + 1;
    }
    return nums;
}

int main() {
    srand(time(NULL));
    unsigned int number;
    printf("Please enter a number: ");
    scanf("%d", &number);
    int* x = getRandomNums(number);
   


    for (int j = 0; j < number; j++) {
        printf("%d\n", *(x+j));
    }

    if (x) {
        free(x);
        x = NULL;
    }

    return 0;
} 