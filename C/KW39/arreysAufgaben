int main(int argc, char** argv) {
    int sui [7]=
    {7, 129, -3, 64, -11, 35, 22, 40};
    int temp;
    int test = sizeof (sui);
    
    printf("%d", sui[0]);
    
    if (sui [0] > sui [1]) {
        temp = sui [0];
        sui [0] = sui [1];
        sui [1] = temp;

    }
    
    printf ("%d", sui[0]);




    return (EXIT_SUCCESS);

}